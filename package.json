{
  "name": "serverless-plugin-typescript",
  "version": "0.0.0-semantic-release",
  "license": "MIT",
  "main": "build/server/index.js",
  "workspaces": [
    "app",
    "server"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "precommit": "npm run test",
    "build": "rimraf dist && tsc",
    "offline": "cd server && sls offline",
    "deploy:dev": "cd app && yarn run build && cd ../api && sls deploy --stage dev",
    "server": "yarn workspace server dev",
    "servernomon": "yarn workspace server start",
    "app": "yarn workspace app start",
    "start": "concurrently --kill-others-on-fail \"yarn server\"  \"yarn app\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prisma/serverless-plugin-typescript.git"
  },
  "keywords": [
    "serverless",
    "serverless plugin",
    "typescript",
    "aws lambda"
  ],
  "devDependencies": {

  },
  "dependencies": {

  },
  "peerDependencies": {
    "typescript": ">=2.2.2"
  }
}
